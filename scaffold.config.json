    "com.abhishekverma.multirepo": "{{PACKAGE_NAME}}",
    "wf_69157991bfd081909cc0815050b47abf0f93481201224b4b": "{{CHATKIT_WORKFLOW_ID}}",
    "kqxiugkmkvymoegzxoye": "{{SUPABASE_PROJECT_REF}}",
